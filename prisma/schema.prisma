generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model test_table {
  id            Int             @id @default(autoincrement())
  name          String
  email         String          @unique
  another_table another_table[]
}

model another_table {
  id      Int        @id @default(autoincrement())
  ownerId Int
  owner   test_table @relation(fields: [ownerId], references: [id])
  content String
}
